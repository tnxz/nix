cmd - return : open -a "/Applications/kitty.app"
alt - return : open -a "/Applications/kitty.app"
alt - 1 : open -a "/Applications/kitty.app"

alt - 2 : open -a "/Applications/Helium.app"
alt - w : open -a "/Applications/Helium.app"

alt - o : open -a "/Applications/OrbStack.app"
alt - 3 : open -a "/Applications/OrbStack.app"

alt + shift - w : open -a "/Applications/Safari.app"
alt + shift - return : open -a "/System/Applications/Utilities/Terminal.app"
